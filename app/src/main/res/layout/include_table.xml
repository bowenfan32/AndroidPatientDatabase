            android:id="@+id/t3"
            android:text="0"
            android:layout_height="wrap_content"
            android:layout_width="0dip"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/i3"
            android:text="0"
            android:layout_height="wrap_content"
            android:layout_width="0dip"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/m3"
            android:text="0"
            android:layout_height="wrap_content"
            android:layout_width="0dip"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/r3"
            android:text="0"
            android:layout_height="wrap_content"
            android:layout_width="0dip"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/p3"
            android:text="0"
            android:layout_height="wrap_content"
            android:layout_width="0dip"
            android:layout_weight="1"
            />
    </TableRow>
</TableLayout>